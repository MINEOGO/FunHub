local Tab = Window:CreateTab("Script here :) ", 4483362458) -- Title, Image

local jumpConnection = nil  -- To store the connection for disabling later

local Toggle = Tab:CreateToggle({
   Name = "inf jump!",
   CurrentValue = false,
   Flag = "Toggle1", -- A flag is the identifier for the configuration file
   Callback = function(Value)
       if Value then
           -- Start infinite jump when toggle is on
           local function infiniteJump()
               local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
               local gravity = workspace.Gravity

               -- Store the connection to disconnect later if needed
               jumpConnection = game:GetService("UserInputService").JumpRequest:Connect(function()
                   humanoid:ChangeState("Jumping")
               end)
           end

           infiniteJump() -- Start infinite jump

       else
           -- Disable infinite jump when toggle is off
           if jumpConnection then
               jumpConnection:Disconnect()  -- Stop the jump function
               jumpConnection = nil  -- Reset the connection
               print("Infinite jump disabled")  -- For debugging purposes
           end
       end
   end,
})

local Slider = Tab:CreateSlider({
   Name = "Slider Example",
   Range = {0, 169},
   Increment = 10,
   Suffix = "niggers", -- Changed to a more appropriate suffix
   CurrentValue = 10,
   Flag = "Slider1", -- A flag is the identifier for the configuration file
   Callback = function(Value)
       local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
       humanoid.WalkSpeed = Value  -- Corrected: use 'Value' here
   end,
})

local Toggle = Tab:CreateToggle({
   Name = "Slickback!!/160 speed and inf jump",
   CurrentValue = false,
   Flag = "Toggle2", -- A flag is the identifier for the configuration file
   Callback = function(Value)
       local jumpConnection = nil  -- To store the connection for disabling later
       local originalWalkSpeed = 16  -- Default walk speed before toggle
       local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
       
       if Value then
           -- When toggle is turned on, set walk speed and start infinite jump
           humanoid.WalkSpeed = 160  -- Change walk speed to 160

           -- Infinite jump functionality
           jumpConnection = game:GetService("UserInputService").JumpRequest:Connect(function()
               humanoid:ChangeState("Jumping")
           end)
       else
           -- When toggle is turned off, reset walk speed and stop infinite jump
           humanoid.WalkSpeed = originalWalkSpeed  -- Reset walk speed to original value
           
           if jumpConnection then
               jumpConnection:Disconnect()  -- Disconnect the jump request listener
               jumpConnection = nil  -- Reset the connection
           end
       end
   end,
})


local Button = Tab:CreateButton({
   Name = "floating!",
   Callback = function()
   -- Place this script in a LocalScript inside StarterPlayerScripts

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")
local humanoid = character:WaitForChild("Humanoid")

-- Create ScreenGui
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "FloatControlGui"

-- Create Main Frame (Draggable)
local controlFrame = Instance.new("Frame", gui)
controlFrame.Size = UDim2.new(0, 160, 0, 90)
controlFrame.Position = UDim2.new(0.5, -80, 0.7, 0)
controlFrame.BackgroundTransparency = 0.2
controlFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
controlFrame.Active = true
controlFrame.Draggable = true

-- Create Title Bar for dragging
local titleBar = Instance.new("TextLabel", controlFrame)
titleBar.Size = UDim2.new(1, 0, 0.3, 0)
titleBar.Text = "Made by Mineogo"
titleBar.TextColor3 = Color3.new(1, 1, 1)
titleBar.BackgroundTransparency = 0.3
titleBar.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
titleBar.Font = Enum.Font.SourceSans
titleBar.TextSize = 16

-- Create Up Button
local upButton = Instance.new("TextButton", controlFrame)
upButton.Size = UDim2.new(0.3, 0, 0.7, 0)
upButton.Position = UDim2.new(0, 0, 0.3, 0)
upButton.Text = "⬆"
upButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)

-- Create Down Button
local downButton = Instance.new("TextButton", controlFrame)
downButton.Size = UDim2.new(0.3, 0, 0.7, 0)
downButton.Position = UDim2.new(0.7, 0, 0.3, 0)
downButton.Text = "⬇"
downButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)

-- Create Stop Button
local stopButton = Instance.new("TextButton", controlFrame)
stopButton.Size = UDim2.new(0.4, 0, 0.7, 0)
stopButton.Position = UDim2.new(0.3, 0, 0.3, 0)
stopButton.Text = "STOP"
stopButton.BackgroundColor3 = Color3.fromRGB(255, 255, 0)

-- Floating mechanics
local floatForce = 5
local bodyPosition

local function createBodyPosition()
    bodyPosition = Instance.new("BodyPosition")
    bodyPosition.MaxForce = Vector3.new(0, math.huge, 0)
    bodyPosition.Position = rootPart.Position
    bodyPosition.Parent = rootPart
end

local function startFloating()
    if not bodyPosition then
        createBodyPosition()
    end
end

local function stopFloating()
    if bodyPosition then
        bodyPosition:Destroy()
        bodyPosition = nil
    end
end

-- Button Actions
upButton.MouseButton1Down:Connect(function()
    startFloating()
    bodyPosition.Position = rootPart.Position + Vector3.new(0, floatForce, 0)
end)

downButton.MouseButton1Down:Connect(function()
    startFloating()
    bodyPosition.Position = rootPart.Position - Vector3.new(0, floatForce, 0)
end)

stopButton.MouseButton1Click:Connect(function()
    stopFloating()
end)

-- Reset when character respawns
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    rootPart = character:WaitForChild("HumanoidRootPart")
    humanoid = character:WaitForChild("Humanoid")
    stopFloating()
end)

-- Keep hands down (Removed SetStateEnabled line to fix mobile jump button)
-- humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, false) -- Removed to prevent interference
   end,
})

local Button = Tab:CreateButton({
   Name = "tp tool!",
   Callback = function()
   -- Place this script inside a Tool in StarterPack

local tool = Instance.new("Tool")
tool.Name = "by mineogo!"
tool.RequiresHandle = false
tool.Parent = game.Players.LocalPlayer.Backpack

local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local character = player.Character or player.CharacterAdded:Wait()

local isToolActive = false  -- Flag to track if tool is active

-- Function to teleport player when tool is activated (used)
tool.Activated:Connect(function()
    isToolActive = true
end)

-- Function to handle mouse click and teleportation
mouse.Button1Down:Connect(function()
    if isToolActive then
        -- Get the position of the mouse click
        local targetPosition = mouse.Hit.p
        local targetPart = mouse.Target

        -- Check if the target is a valid surface (BasePart)
        if targetPart and targetPart:IsA("BasePart") then
            -- Teleport the player to the clicked position
            character:SetPrimaryPartCFrame(CFrame.new(targetPosition))
        end
    end
end)

-- Deactivate the tool when unequipped
tool.Unequipped:Connect(function()
    isToolActive = false
end)
   end,
})

local Label = Tab:CreateLabel("to use tp tool you have to click where you want to teleport!", circle-check-big, Color3.fromRGB(255, 255, 255), false) -- Title, Icon, Color, IgnoreTheme

local Button = Tab:CreateButton({
   Name = "TOUCH FLING(NOT MINE!)",
   Callback = function()
-- Touch fling gui

-- Gui to Lua (VIP VERSION)
-- Version: 6.9

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
